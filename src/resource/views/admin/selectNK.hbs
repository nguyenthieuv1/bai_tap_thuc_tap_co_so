<div class="container">
    <h1 class="h1 text-center">Chọn liên khóa</h1>
    <form action="" method="post">
        <div class="mb-3">
            <label for="nienkhoa" class="form-label">Niên khóa</label>
            <select class="form-select" id="nienkhoa" name="nienkhoa">
                {{#nienkhoa}}
                <option>{{this.ten}}</option>
                {{/nienkhoa}}
            </select>
        </div>
        <button class="btn btn-primary">Xác nhận</button>
    </form>
</div>